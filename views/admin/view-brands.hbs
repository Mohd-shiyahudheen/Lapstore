<div class="container">
    <div class="row">
        <div class="col-md-12">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">No</th>
                        <th scope="col">Brand name</th>
                        <th scope="col">view</th>

                    </tr>
                </thead>
                <tbody>
                    {{#each brand}}
                    <tr>
                        <td>{{@index}}</td>
                        <td>{{this.brandname}}</td>
                        <td>
                            <a href="/admin/delete-brand/{{this._id}}"><i class='bx bx-trash fs-3 ml-3 text-danger'></i></a>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>

        </div>

    </div>
</div>